    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^3.0.0-beta.20",
    "@ntnyq/prettier-config": "^1.21.3",
    "@type-challenges/utils": "^0.1.1",
    "@types/prompts": "^2.4.9",
    "eslint": "^9.12.0",
    "husky": "^9.1.6",
    "nano-staged": "^0.8.0",
    "picocolors": "^1.1.0",
    "prettier": "^3.3.3",
    "prompts": "^2.4.2",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.8",
    "vitepress": "^1.4.1",
    "vitest": "^2.1.2"
  },
  "prettier": "@ntnyq/prettier-config",
  "nano-staged": {
    "*.{js,ts,cjs,mjs,json,md,yaml,yml}": "eslint --fix"
