  "prettier": "@ntnyq/prettier-config",
  "devDependencies": {
    "@ntnyq/prettier-config": "^1.4.2",
    "@type-challenges/utils": "^0.1.1",
    "@types/prompts": "^2.4.2",
    "husky": "^8.0.3",
    "nano-staged": "^0.8.0",
    "picocolors": "^1.0.0",
    "prettier": "^2.8.4",
    "prompts": "^2.4.2",
    "rimraf": "^4.4.0",
    "tsx": "^3.12.3",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vitepress": "1.0.0-alpha.51"
  },
  "nano-staged": {
    "*.{js,ts,json,md,yaml,yml}": "prettier --write"
  }
