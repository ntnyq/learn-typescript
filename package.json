{
  "name": "learn-typescript",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@9.12.1",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "dev": "tsc --watch",
